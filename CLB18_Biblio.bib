@article{korzycka2015,
  title = {Two-Photon Sensitive Protecting Groups Operating via Intramolecular Electron Transfer: Uncaging of {{GABA}} and Tryptophan},
  shorttitle = {Two-Photon Sensitive Protecting Groups Operating via Intramolecular Electron Transfer},
  author = {Korzycka, Karolina A. and Bennett, Philip M. and {Cueto-Diaz}, Eduardo Jose and Wicks, Geoffrey and Drobizhev, Mikhail and {Blanchard-Desce}, Mireille and Rebane, Aleksander and Anderson, Harry L.},
  year = {2015},
  volume = {6},
  pages = {2419--2426},
  issn = {2041-6520, 2041-6539},
  doi = {10.1039/C4SC03775H},
  abstract = {We present a modular approach to photo-labile protecting groups based on photoinduced electron transfer, providing high sensitivity to two-photon excitation.           ,              Improved photo-labile protecting groups, with high sensitivity to two-photon excitation, are needed for the controlled release of drugs, as tools in neuroscience and physiology. Here we present a new modular approach to the design of caging groups based on photoinduced electron transfer from an electron-rich two-photon dye to an electron acceptor, followed by scission of an ester to release a carboxylic acid. Three different electron acceptors were tested: nitrobenzyl, phenacyl and pyridinium. The nitrobenzyl system was ineffective, giving only photochemical decomposition and no release of the carboxylic acid. The phenacyl system also performed poorly, liberating the carboxylic acid in 20\% chemical yield and 0.2\% photochemical yield. The pyridinium system was most successful, and was tested for the release of two carboxylic acids: {$\gamma$}-amino butyric acid (GABA) and tryptophan. The caged GABA undergoes photochemical cleavage with a chemical yield of {$>$}95\% and a photochemical yield of 1\%; it exhibits a two-photon absorption cross section of 1100 GM at 700 nm, corresponding to a two-photon uncaging cross section of 10 {$\pm$} 3 GM.},
  file = {/Users/pierpa/Zotero/storage/NRCXBYUP/c4sc03775h.pdf;/Users/pierpa/Zotero/storage/RWDKGR8Z/Korzycka et al. - 2015 - Two-photon sensitive protecting groups operating v.pdf},
  journal = {Chem. Sci.},
  language = {en},
  number = {4}
}

