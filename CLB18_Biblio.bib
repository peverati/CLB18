@article{mingtong2015,
  title = {Theoretical Studies on the Photophysical Properties of Luminescent Pincer Gold( {\textsc{Iii}} ) Arylacetylide Complexes: The Role of {$\pi$}-Conjugation at the {{C}}-Deprotonated [{{C}}\^{{N}}\^{{C}}] Ligand},
