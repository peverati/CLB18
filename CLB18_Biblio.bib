  author = {Smith, Daniel and Burns, Lori and Simmonett, Andrew and Parrish, Robert and Schieber, Matthew and Galvelis, Raimondas and Kraus, Peter and Kruse, Holger and Di Remigio, Roberto and Alenaizan, Asem and James, Andrew and Lehtola, Susi and Misiewicz, Jonathon and Scheurer, Maximilian and Shaw, Robert and Schriber, Jeffrey and Xie, Yi and Glick, Zachary and Sirianni, Dominic and O'Brien, Joseph and Waldrop, Jonathan and Kumar, Ashutosh and Hohenstein, Edward G. and Pritchard, Benjamin and Brooks, Bernard and Schaefer, Henry and Sokolov, Alexander and Patkowski, Konrad and DePrince, Eugene and Bozkaya, Ugur and King, Rollin and Evangelista, Francesco and Turney, Justin and Crawford, Thomas and Sherrill, David},
  year = {2020},
  month = mar,
  doi = {10.26434/chemrxiv.11930031.v1},
  abstract = {{$<$}div{$>$} 			{$<$}div{$>$} 				{$<$}div{$>$} 					{$<$}p{$>$}Psi4 is a free and open-source ab initio electronic structure program providing Hartree\textendash Fock, density functional theory, many-body perturbation theory, configuration interaction, density cumulant theory, symmetry-adapted perturbation theory, and coupled-cluster theory. Most of the methods are quite efficient thanks to density fitting and multi-core parallelism. The program is a hybrid of C++ and Python, and calculations may be run with very simple text files or using the Python API, facilitating post-processing and complex workflows; method developers also have access to most of Psi4's core functionality via Python. Job specification may be passed using The Molecular Sciences Software Institute (MolSSI) QCSchema data format, facilitating interoperability. A rewrite of our top-level computation driver, and concomitant adoption of the MolSSI QCArchive Infrastructure project, make the latest version of Psi4 well suited to distributed computation of large numbers of independent tasks. The project has fostered the development of independent software components that may be reused in other quantum chemistry programs. {$<$}/p{$>$} 				{$<$}/div{$>$} 			{$<$}/div{$>$} 		{$<$}/div{$>$}},
  type = {Preprint}
}

